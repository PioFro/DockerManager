@model DockerWebApplication.Models.Docker

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Docker</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LastUsedDate" class="control-label"></label>
                <input asp-for="LastUsedDate" class="form-control" />
                <span asp-validation-for="LastUsedDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PullName" class="control-label"></label>
                <input asp-for="PullName" class="form-control" />
                <span asp-validation-for="PullName" class="text-danger"></span>
            </div>
            @{Html.RenderPartial("EnvironmentalVariablesList", Model.v); }
            @{Html.RenderPartial("Resource", Model.resources); }

            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

